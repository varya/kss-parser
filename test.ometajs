ometa kssDivider {

  inlineComment = seq('//') (~seq('\n') char)*:x seq('\n') -> x.join(''),

  kssMarkup = inlineComment (~seq('\n\n') char)*:x seq('\n\n') -> [#kss, x.join('')],

  blockEnd = kssMarkup | end,

  nl = seq('\n\n'),

  block = kssMarkup:k (~blockEnd char)*:c blockEnd -> [k, [#code, c.join('')]],

  any = (block)*

}
